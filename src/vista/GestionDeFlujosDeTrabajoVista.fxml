<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.GestionDeFlujosDeTrabajoControlador">
   <children>
      <Label layoutX="221.0" layoutY="14.0" text="Gestion Flujos De Trabajo">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="227.0" layoutY="46.0" text="Flujos De Trabajo Existentes" />
      <ListView fx:id="listaFlujosExistentes" layoutX="146.0" layoutY="69.0" prefHeight="110.0" prefWidth="308.0" />
      <Button fx:id="buttonCrearFlujo" layoutX="216.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleCrearFlujoDeTrabajo" text="Crea Nuevo Flujo De Trabajo" />
      <Button fx:id="buttonModFlujo" layoutX="217.0" layoutY="234.0" mnemonicParsing="false" onAction="#handleModificarFlujoDeTrabajo" prefHeight="25.0" prefWidth="168.0" text="Modificar Flujo De Trabajo" />
      <Button fx:id="buttonEliminarFlujo" layoutX="216.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleEliminarFlujoDeTrabajo" prefHeight="25.0" prefWidth="168.0" text="Eliminar Flujo De Trabajo" />
